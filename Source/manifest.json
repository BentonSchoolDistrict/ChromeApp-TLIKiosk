{
"update_url": "https://clients2.google.com/service/update2/crx",


  "name": "BPSD - TLI Kiosk",
  "version": "1.0.0.9",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png"
  },
  "app": {
    "background": {
      "scripts": [ "background.js" ],
      "persistent": false
    }
  },
  "permissions": [
    "webview",
	"fullscreen"
  ],
  "kiosk_enabled": true
}